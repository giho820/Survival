<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_checklist_import_list_click_area"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/item_checklist_import_header" />

    <LinearLayout
        android:id="@+id/linearlayout_checklist_import_add"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_43.5"
        android:gravity="center_vertical"
        android:orientation="horizontal">


        <com.survivalsos.goldentime.common.view.LatoBlackTextView
            android:id="@+id/tv_checklist_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:text="항목의 이름입니다"
            android:textSize="@dimen/dp_14"
            android:textColor="@color/c_ffffffff"/>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"/>


        <LinearLayout
            android:layout_width="@dimen/dp_50"
            android:layout_height="match_parent"
            android:gravity="center">
            <ImageView
                android:id="@+id/iv_checklist_trash_icon"
                android:layout_width="@dimen/dp_25"
                android:layout_height="@dimen/dp_25"
                android:background="@drawable/check_list_import_check_all_00"
                />
        </LinearLayout>
    </LinearLayout>



</LinearLayout>