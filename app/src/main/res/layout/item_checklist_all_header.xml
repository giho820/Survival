<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearlayout_checklist_all_header"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_33.5"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <com.survivalsos.goldentime.common.view.textview.NanumBarunGothicTextView
            android:id="@+id/tv_checklist_title_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_13.5"
            android:text="체크리스트의 항목에 대한 헤더입니다"
            android:textSize="@dimen/dp_10" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/item_line_checklist_header"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/c_ffD0D0D0"
        android:orientation="vertical"
        android:visibility="visible"></LinearLayout>
</LinearLayout>